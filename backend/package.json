{
  "name": "backend",
  "version": "1.0.0",
  "description": "server side",
  "main": "index.js",
  "scripts": {
    "type": "module",
    "testAuth": "jest --detectOpenHandles --forceExit --coverage --coverageDirectory=coverage/auth src/test/auth.test.js",
    "testComment": "jest  --forceExit --coverage --coverageDirectory=coverage/comments src/test/comments.test.js",
    "testSong": "jest  --forceExit --coverage --coverageDirectory=coverage/song src/test/song.test.js",
    "testOrder": "jest --forceExit --coverage --coverageDirectory=coverage/order src/test/order.test.js",
    "testUser": "jest --forceExit --coverage --coverageDirectory=coverage/order src/test/user.test.js",
    "aggregateCoverage": "jest --coverage --maxWorkers=1 --coverageDirectory=coverage --collectCoverageFrom=src/**/*.js --forceExit",
    "test": "npm run testComment && npm run testAuth && npm run testOrder && npm run testSong ",
    "test1": "npm run testComment &&  npm run aggregateCoverage",
    "start": "nodemon src/index.js"
  },
